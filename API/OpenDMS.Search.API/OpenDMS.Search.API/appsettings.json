{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  // Lasciare invariati dati qui sotto
  "Keycloak": {
    "realm": "master",
    "auth-server-url": "https://localhost:18080/auth",
    "ssl-required": "external",
    "resource": "dms",
    "verify-token-audience": false,
    "use-resource-role-mappings": true,
    "credentials": {
      "secret": "<enter secret here>"
    },
    "confidential-port": 0,
    "policy-enforcer": {}
  },
  "Documents": {
    "RootFolder": "z:\\archivio\\",
    "RecoverableDelete": true,
    "FileSystemType": "",
    "FilePrefix": "dms",
    "Protocol": {
      "Length": 7
    }
  },
  "TenantResolver": "none",
  "Provider": "mysql"
}
