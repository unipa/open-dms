{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "RabbitMQ_Uri": "amqp://guest:guest@localhost:5672",
  "RabbitMQ_Notification_Queue": "opendms_notifications",
  "RabbitMQ_Mail_Queue": "opendms_mail",

  "Documents": {
    "RootFolder": "z:\\archivio\\",
    "RecoverableDelete": true,
    "FileSystemType": "",
    "FilePrefix": "dms",
    "Protocol": {
      "Length": 7
    }
  },
  "Endpoint": {
    "AdminService": "https://localhsot:7001/internalapi/admin",
    "DocumentService": "https://localhost:7002",
    "UserService": "https://localhost:7003",
    "DocumentPreviewService": "https://localhost:7004",
    "UIService": "https://localhost:7006",
    "SearchService": "https://localhost:7006",
    "TaskList": "https://localhost:7007"
  },
  "Mail": {
    "Sender": {
      "TimeOut": 5,
      "Frequency": 5,
      "MaxSent": 100
    },
    "Reader": {
      "TimeOut": 5,
      "Frequency": 5,
      "MaxRead": 10
    }
  },
  "MachineName": "Test",
  "TenantResolver": "none",
  "Provider": "mysql",
  "URL_HOST": "localhost:7000",
  "PATH_BASE": "/dpm/amministrazione",
  "ConnectionString": "server=localhost;Database=OpenDMS;uid=root;pwd="

}
