@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration
@using OpenDMS.Domain.Entities;
@using Web.DTOs;
@model UserAuthorizations_DTO
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Strutture";
    Layout = "_CustomizeLayout";
}
<script>

    $(document).ready(function () {
        $(function () {
            Carica("liCustomize-Strutture");
        });
    })
</script>

<div class="MainPanel">
    <div class="Header">
        <a href="/Customize/Index" class="Desktop"> <i class="fa fa-bars"></i></a>
        <h1>
            <small>Profilo Utente</small>
            <span>Strutture</span>
        </h1>
    </div>


    <div id="Message_Target"></div>
    <div class="Panel">
        <ul class="nav nav-list disabled">
            @if (Model.Groups.Count > 0)
            {
                <li class="header">
                    <h6>Strutture</h6>
                </li>
                <li>
                @foreach (var r in Model.Groups)
                {
                    <div class="Row">
                        <span>@r.Description</span>
                        <span class="pull-right">@r.Id</span>
                    </div>
                }
                </li>
            }
        </ul>
    </div>
</div>
